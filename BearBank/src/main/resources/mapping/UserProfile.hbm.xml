<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.Revature.pojos.UserProfile">
		<id name="username" column="USERNAME"></id>
		<property name="password" column="PWD"></property>
		<bag name="accounts" table="USER_PROFILE_BANK_ACCOUNT">
			<key>
				<column name="USERNAME" not-null="true"></column>
			</key>
			<many-to-many entity-name="com.Revature.pojos.BankAccount">
				<column name="ACCOUNT_NUMBER" not-null="true"></column>
			</many-to-many>
		</bag>
	</class>
</hibernate-mapping>