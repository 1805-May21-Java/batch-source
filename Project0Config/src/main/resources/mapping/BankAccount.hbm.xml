<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.pojos.BankAccount">
		<id name="bankID" column="ACCOUNT_ID">
			<generator class="increment"></generator>
		</id>
		<property name="balence" type="org.hibernate.type.DoubleType"></property>
		<property name="accountName" type="org.hibernate.type.StringType"></property>
		<property name="email" type="org.hibernate.type.Stringtype"></property>
	</class>

</hibernate-mapping>