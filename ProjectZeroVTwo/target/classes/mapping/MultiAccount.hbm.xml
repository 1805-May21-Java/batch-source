<hibernate-mapping>
	<class name="com.revature.pojos.MultiAccount">
		<id name="username" column="USERNAME"/>
		<id name="accountType" column="ACCT_TYPE"/>
		<property name="balance" column="BALANCE"/>
	</class>
</hibernate-mapping>